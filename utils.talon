scratch:
    key(ctrl-w)

# Keys

lud:
    insert("à")

lid:
    insert("ò")

# Terminal Commands

stack new:
    insert("stack new ")

edit:
   insert("nvim ")

# Desktop Navigation

term:
    key(super-shift-enter)
terminate:
    key(super-shift-c)
